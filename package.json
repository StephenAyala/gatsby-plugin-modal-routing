{
  "name": "gatsby-plugin-modal-routing",
  "version": "2.0.0",
  "description": "Gatsby plugin to enable routing of modal pages",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --ignore **/__tests__",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "prepare": "NODE_ENV=production npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "modal",
    "routing"
  ],
  "files": [
    "/*.js",
    "/lib"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.13.15",
    "babel-preset-gatsby-package": "^1.2.0"
  }
}
